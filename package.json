{
    "name": "wego-monorepo",
    "description": "Monorepo for Wego",
    "scripts": {
        "dev:bot": "pnpm --filter wego-bot dev",
        "build:bot": "pnpm --filter wego-bot dev",
        "build:logger": "pnpm --filter @wego/logger build",
        "lint:typescript": "pnpm --stream --parallel lint:typescript",
        "lint:eslint": "pnpm --stream --parallel lint:eslint",
        "ci:release": "pnpm -r --workspace-concurrency=1 exec -- semantic-release -e semantic-release-monorepo",
        "ci:dry-release": "pnpm -r --workspace-concurrency=1 exec -- semantic-release -e semantic-release-monorepo --dry-run"
    },
    "license": "UNLICENSED",
    "devDependencies": {
        "@rollup/plugin-commonjs": "25.0.7",
        "@rollup/plugin-json": "6.1.0",
        "@rollup/plugin-typescript": "11.1.6",
        "@semantic-release/changelog": "6.0.3",
        "@semantic-release/commit-analyzer": "9.0.2",
        "@semantic-release/exec": "6.0.3",
        "@semantic-release/git": "10.0.1",
        "@semantic-release/github": "9.0.4",
        "@semantic-release/release-notes-generator": "11.0.4",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "commitizen": "4.3.0",
        "cross-env": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.57.0",
        "lint-staged": "15.2.2",
        "nodemon": "3.1.0",
        "prettier": "3.2.5",
        "rollup": "4.14.1",
        "rollup-plugin-node-externals": "5.1.3",
        "rollup-plugin-replace": "2.2.0",
        "semantic-release": "21.0.2",
        "semantic-release-monorepo": "^8.0.2",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.4.5"
    },
    "dependencies": {
        "@aws-sdk/client-comprehend": "3.556.0",
        "@aws-sdk/client-sqs": "3.569.0",
        "@discord-player/extractor": "4.4.7",
        "@discordjs/opus": "0.9.0",
        "@discordjs/rest": "2.2.0",
        "@discordjs/voice": "0.16.1",
        "ajv": "8.12.0",
        "bad-words": "3.0.4",
        "bunyan": "1.8.15",
        "dayjs": "1.11.10",
        "discord-api-types": "0.37.79",
        "discord-player": "6.6.8",
        "discord.js": "14.14.1",
        "dotenv": "16.4.5",
        "extract-domain": "2.4.8",
        "extract-urls": "1.4.1",
        "ffmpeg-static": "5.2.0",
        "i18n": "0.15.1",
        "jimp": "0.22.12",
        "knex": "3.1.0",
        "mysql2": "3.9.7",
        "node-cron": "3.0.3",
        "objection": "3.1.4",
        "redis": "4.6.13",
        "reflect-metadata": "0.2.2",
        "soundcloud-scraper": "5.0.3",
        "spotify-url-info": "3.2.13",
        "text-table": "0.2.0",
        "tsyringe": "4.8.0",
        "youtube-sr": "4.3.11",
        "ytdl-core": "4.11.5"
    },
    "lint-staged": {
        "*.{ts,js}": [
            "eslint --cache --quiet --fix",
            "prettier --write"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
