{
    "name": "wego-bot",
    "version": "3.7.1",
    "description": "",
    "main": "dist/app.js",
    "scripts": {
        "dev": "pnpm nodemon --watch src --watch 'src/**/*' -e ts,json --exec \"pnpm build && pnpm start | bunyan\"",
        "dev:no-watch": "pnpm build && pnpm start | bunyan",
        "knex": "pnpm cross-env TS_NODE_PROJECT=tsconfig.knex.json knex --knexfile ./knexfile.ts",
        "build": "rollup -c --bundleConfigAsCjs",
        "start": "node dist/app.js",
        "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:typescript": "pnpm tsc --noEmit --skipLibCheck"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/bad-words": "3.0.3",
        "@types/bunyan": "1.8.11",
        "@types/i18n": "0.13.10",
        "@types/node": "18.11.18",
        "@types/node-cron": "3.0.11",
        "@types/node-fetch": "2.6.11",
        "@types/text-table": "0.2.5",
        "tslib": "^2.6.2"
    },
    "dependencies": {
        "@aws-sdk/client-comprehend": "3.525.0",
        "@aws-sdk/client-sqs": "3.525.0",
        "@discord-player/extractor": "4.4.6",
        "@discordjs/opus": "0.9.0",
        "@discordjs/rest": "2.2.0",
        "@discordjs/voice": "0.16.1",
        "@wego/logger": "workspace:*",
        "ajv": "8.12.0",
        "bad-words": "3.0.4",
        "bunyan": "1.8.15",
        "dayjs": "1.11.10",
        "discord-api-types": "0.37.71",
